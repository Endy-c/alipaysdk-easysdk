{
    "name": "evit/php-alipaysdk-easysdk",
    "description": "支付宝官方 Alipay Easy SDK|这是从官方包fork出来的项目，一切原有代码版权均归官方所有|官方地址：https://github.com/alipay/alipay-easysdk/",
    "type": "library",
    "authors": [
        {
            "name": "junying.wjy, Endy",
            "email": "junying.wjy@antfin.com, endy@evit.net.cn"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alipay\\EasySDK\\": "php/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alipay\\EasySDK\\Test\\": "php/test/"
        }
    },
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.0",
        "ext-curl": "*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-xmlwriter": "*",
        "guzzlehttp/guzzle": ">=6.3",
        "alibabacloud/tea": "^3.1",
        "alibabacloud/tea-fileform": "^0.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}